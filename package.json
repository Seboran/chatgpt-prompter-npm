{
  "name": "chatgpt-prompter",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "rimraf dist && tsc && uglifyjs dist/*.js --compress --mangle --output dist/app.min.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'npm run test && npm run build'",
    "format": "prettier --write \"src/**/*.ts\"",
    "precommit": "yarn format && yarn test && git add .",
    "start": "node dist/app.min.js"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2",
    "uglify-js": "^3.17.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "dependencies": {
    "i18next": "^22.4.13",
    "i18next-node-fs-backend": "^2.1.3"
  }
}
